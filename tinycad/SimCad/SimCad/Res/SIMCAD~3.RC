    MENUITEM "&Grid\tCtrl+G",IDM_VIEW_GRID,,
    MENUITEM "&StatusBar",IDM_VIEW_STATUSBAR,,
    MENUITEM "",,0x00000800,
    MENUITEM "Zoom &In\tPgDn",IDM_VIEW_ZOOM_IN,,
    MENUITEM "Zoom &Out\tPgUp",IDM_VIEW_ZOOM_OUT,,
    MENUITEM "Zoom &Best Fit\tCtrl+B",IDM_VIEW_ZOOM_BEST_FIT,,
  END
  POPUP "&Tools",,,
  BEGIN
    MENUITEM "(Empty)",11000,,0x00000001
  END
  POPUP "&Option",,,
  BEGIN
    MENUITEM "&Setup",IDM_OPTION_SETUP,,0x00000001
    MENUITEM "",,0x00000800,
    MENUITEM "&Tools Menu",IDM_OPTION_TOOLS_MENU,,0x00000001
    MENUITEM "&Help Menu",IDM_OPTION_HELP_MENU,,0x00000001
  END
  POPUP "&Help",,,
  BEGIN
    MENUITEM "&About",IDM_HELP_ABOUT,,
    MENUITEM "",,0x00000800,
    MENUITEM "(Empty)",12000,,0x00000001
  END
