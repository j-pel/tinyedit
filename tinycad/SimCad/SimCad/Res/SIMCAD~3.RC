#define IDM_FILE_SAVE_AS 10108
#define IDM_FILE_PAGESETUP 10110
#define IDM_FILE_PRINT 10004
#define IDM_FILE_EXIT 10001
#define IDM_EDIT_UNDO 10101
#define IDM_EDIT_REDO 10102
#define IDM_EDIT_CUT 10103
#define IDM_EDIT_COPY 10104
#define IDM_EDIT_PASTE 10105
#define IDM_EDIT_DELETE 10106
#define IDM_EDIT_SELECTALL 10109
#define IDM_EDIT_COLOR 10120
#define IDM_EDIT_WIDTH 10121
#define IDM_EDIT_SNAP 10119
#define IDM_VIEW_GRID 10111
#define IDM_VIEW_STATUSBAR 10113
#define IDM_VIEW_ZOOM_IN 10115
#define IDM_VIEW_ZOOM_OUT 10116
