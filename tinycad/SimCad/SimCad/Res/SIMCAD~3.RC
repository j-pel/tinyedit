#define IDM_OPTION_SETUP 10112
#define IDM_OPTION_TOOLS_MENU 10114
#define IDM_OPTION_HELP_MENU 10118
#define IDM_HELP_ABOUT 10901
IDM_MAIN MENUEX
BEGIN
  POPUP "&File",,,
  BEGIN
    MENUITEM "&New\tCtrl+N",IDM_FILE_NEW,,
    MENUITEM "&Open\tCtrl+O",IDM_FILE_OPEN,,
    MENUITEM "&Save\tCtrl+S",IDM_FILE_SAVE,,
    MENUITEM "Save &As\tShift+Ctrl+S",IDM_FILE_SAVE_AS,,
    MENUITEM "",,0x00000800,
    MENUITEM "Page Setup\tShift+Ctrl+P",IDM_FILE_PAGESETUP,,
    MENUITEM "&Print\tCtrl+P",IDM_FILE_PRINT,,
    MENUITEM "",,0x00000800,
    MENUITEM "E&xit\tAlt+F4",IDM_FILE_EXIT,,
  END
  POPUP "&Edit",,,
  BEGIN
    MENUITEM "&Undo\tCtrl+Z",IDM_EDIT_UNDO,,
    MENUITEM "&Redo\tCtrl+Y",IDM_EDIT_REDO,,
    MENUITEM "",,0x00000800,
    MENUITEM "&Cut\tCtrl+X",IDM_EDIT_CUT,,
    MENUITEM "C&opy\tCtrl+C",IDM_EDIT_COPY,,
    MENUITEM "&Paste\tCtrl+V",IDM_EDIT_PASTE,,
    MENUITEM "&Delete\tDel",IDM_EDIT_DELETE,,
    MENUITEM "",,0x00000800,
    MENUITEM "&Select All\tCtrl+A",IDM_EDIT_SELECTALL,,
    MENUITEM "",,,
    MENUITEM "Set C&olor",IDM_EDIT_COLOR,,
    MENUITEM "Set &Width",IDM_EDIT_WIDTH,,
    MENUITEM "Snap To &Grid",IDM_EDIT_SNAP,,
  END
  POPUP "&View",,,
